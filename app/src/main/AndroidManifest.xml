<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.wkweb.com.vmart">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FarmerBillShowPdfActivity"></activity>
        <activity android:name=".NewPdfFarmerbillActivity" />
        <activity android:name=".RatelistActivity" />
        <activity android:name=".FarmerItemReportActivity" />
        <activity android:name=".FarmerBillListActivity" />
        <activity android:name=".FarmerBillActivity2" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SiginupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OTPActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OtpVerifyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ChangePasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyAddressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddmyAddressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ReviewOrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OrderlistActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LocationsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TodaysPricesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CustomersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CartitemActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CartCustomerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PatnerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AboutUsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LocationInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StockAvailabelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VenderDeliveryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FarmerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FarmerOrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HistoryFarmerActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <service
            android:name=".MyFirebaseInstanceIdServices"
            android:enabled="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessengingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>